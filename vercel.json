{
  "version": 2,
  "functions": {
    "api/*.js": {
      "memory": 128,
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/api",
      "dest": "/api/index.js"
    },
    {
      "src": "/api/image",
      "dest": "/api/image.js"
    },
    {
      "src": "/",
      "dest": "/api/index.js"
    }
  ]
}